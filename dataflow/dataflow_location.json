{
	"name": "dataflow_location",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_parquet_2_location",
						"type": "DatasetReference"
					},
					"name": "inputsource"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"parameters{",
				"     DI_JOB_ID as string (\"23\"),",
				"     location_sk as integer (1)",
				"}",
				"source(output(",
				"          state_fips as string,",
				"          state as string,",
				"          state_abbr as string,",
				"          zipcode as string,",
				"          county as string,",
				"          city as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'parquet') ~> inputsource",
				"inputsource derive(DI_JOB_id = ERROR_FUNCTION(''),",
				"          Location_Sk = ERROR_FUNCTION('')) ~> derivedColumn1"
			]
		}
	}
}
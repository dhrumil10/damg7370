{
	"name": "dataflow_location",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_parquet_2_location",
						"type": "DatasetReference"
					},
					"name": "inputsource"
				},
				{
					"dataset": {
						"referenceName": "ds_snowflake_location",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [],
			"scriptLines": [
				"parameters{",
				"     DI_JOB_ID as string (\"23\"),",
				"     location_sk as integer (1)",
				"}",
				"source(output(",
				"          state_fips as string,",
				"          state as string,",
				"          state_abbr as string,",
				"          zipcode as string,",
				"          county as string,",
				"          city as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'parquet') ~> inputsource",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     query: 'select NVL(MAX(Location_sk),0) as  from Staging.LOCATION_DIM;',",
				"     format: 'query') ~> source1"
			]
		}
	}
}
{
	"name": "ds_location",
	"properties": {
		"linkedServiceName": {
			"referenceName": "ls_blob_storage",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"filename": {
				"type": "string",
				"defaultValue": "geo-data.csv"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@dataset().filename",
					"type": "Expression"
				},
				"container": "rawcontainer"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": [
			{
				"name": "state_fips",
				"type": "String"
			},
			{
				"name": "state",
				"type": "String"
			},
			{
				"name": "state_abbr",
				"type": "String"
			},
			{
				"name": "zipcode",
				"type": "String"
			},
			{
				"name": "county",
				"type": "String"
			},
			{
				"name": "city",
				"type": "String"
			}
		]
	}
}